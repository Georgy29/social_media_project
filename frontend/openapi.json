    "/posts/{post_id}/retweet": {
      "post": {
        "tags": ["posts"],
        "summary": "Retweet Post",
        "operationId": "retweet_post_posts__post_id__retweet_post",
        "security": [{ "OAuth2PasswordBearer": [] }],
        "parameters": [
          {
            "name": "post_id",
            "in": "path",
            "required": true,
            "schema": { "type": "integer", "title": "Post Id" }
